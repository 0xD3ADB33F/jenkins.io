.docker-plugins-list
  %h4
    = page.title

  %div.content
    %div.item-list
      %ul.ji-dated-list.ji-item-list.plugins
        - if page.plugins
          - page.plugins.each do |link, title, description|
            %li.post
              %a.body.article{:href => link}
                %h6.title
                  = title
                %p.url
                  = description
                .icon.card.date
                  .icon-plug
